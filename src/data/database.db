-- Buildings =================================================================

CREATE TABLE buildings (
  id INTEGER PRIMARY KEY,
  name TEXT,
  width FLOAT,
  length FLOAT,
  height FLOAT,
  category TEXT,
  subcategory TEXT,
  power FLOAT,
  conveyorIn INTEGER,
  fluidIn INTEGER,
  conveyorOut INTEGER,
  fluidOut INTEGER,
